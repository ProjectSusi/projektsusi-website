{
  "id": null,
  "title": "Docker Containers Overview",
  "tags": ["docker", "containers"],
  "timezone": "browser",
  "schemaVersion": 30,
  "version": 1,
  "refresh": "30s",
  "time": {
    "from": "now-15m",
    "to": "now"
  },
  "panels": [
    {
      "id": 1,
      "title": "Container Status",
      "type": "stat",
      "targets": [
        {
          "expr": "count(up)",
          "legendFormat": "Total Services",
          "refId": "A"
        }
      ],
      "gridPos": {
        "h": 6,
        "w": 8,
        "x": 0,
        "y": 0
      },
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "unit": "none"
        }
      }
    },
    {
      "id": 2,
      "title": "HTTP Requests Rate",
      "type": "timeseries",
      "targets": [
        {
          "expr": "rate(http_requests_total[5m])",
          "legendFormat": "{{method}} {{path}}",
          "refId": "B"
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 16,
        "x": 8,
        "y": 0
      },
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "unit": "reqps"
        }
      }
    },
    {
      "id": 3,
      "title": "Response Times",
      "type": "timeseries",
      "targets": [
        {
          "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket[5m]))",
          "legendFormat": "95th percentile",
          "refId": "C"
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 24,
        "x": 0,
        "y": 8
      },
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "unit": "s"
        }
      }
    }
  ]
}