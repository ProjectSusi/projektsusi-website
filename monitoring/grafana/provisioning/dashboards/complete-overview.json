{
  "id": null,
  "title": "Complete Docker & RAG Overview",
  "tags": ["docker", "rag", "overview", "complete"],
  "timezone": "browser",
  "schemaVersion": 30,
  "version": 1,
  "refresh": "15s",
  "time": {
    "from": "now-30m",
    "to": "now"
  },
  "panels": [
    {
      "id": 1,
      "title": "üê≥ Container Status",
      "type": "stat",
      "targets": [
        {
          "expr": "count(up)",
          "legendFormat": "Total",
          "refId": "A"
        },
        {
          "expr": "count(up == 1)",
          "legendFormat": "Running",
          "refId": "B"
        },
        {
          "expr": "count(up == 0)",
          "legendFormat": "Down",
          "refId": "C"
        }
      ],
      "gridPos": {"h": 4, "w": 12, "x": 0, "y": 0},
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "palette-classic"},
          "unit": "none"
        }
      }
    },
    {
      "id": 2,
      "title": "‚ö° RAG Query Rate",
      "type": "gauge",
      "targets": [
        {
          "expr": "rate(http_requests_total{path=\"/api/v1/query\"}[5m])",
          "legendFormat": "Queries/sec",
          "refId": "D"
        }
      ],
      "gridPos": {"h": 4, "w": 12, "x": 12, "y": 0},
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "thresholds"},
          "unit": "reqps",
          "min": 0,
          "max": 10,
          "thresholds": {
            "steps": [
              {"color": "green", "value": 0},
              {"color": "yellow", "value": 5},
              {"color": "red", "value": 8}
            ]
          }
        }
      }
    },
    {
      "id": 3,
      "title": "üìä HTTP Request Rate by Endpoint",
      "type": "timeseries",
      "targets": [
        {
          "expr": "rate(http_requests_total[5m])",
          "legendFormat": "{{method}} {{path}}",
          "refId": "E"
        }
      ],
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 4},
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "palette-classic"},
          "unit": "reqps"
        }
      }
    },
    {
      "id": 4,
      "title": "‚è±Ô∏è Response Times",
      "type": "timeseries",
      "targets": [
        {
          "expr": "histogram_quantile(0.50, rate(http_request_duration_seconds_bucket[5m]))",
          "legendFormat": "50th percentile",
          "refId": "F"
        },
        {
          "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket[5m]))",
          "legendFormat": "95th percentile",
          "refId": "G"
        }
      ],
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 4},
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "palette-classic"},
          "unit": "s"
        }
      }
    },
    {
      "id": 5,
      "title": "üíæ Database & Cache Status",
      "type": "stat",
      "targets": [
        {
          "expr": "database_connections_active",
          "legendFormat": "DB Connections",
          "refId": "H"
        },
        {
          "expr": "cache_hit_rate * 100",
          "legendFormat": "Cache Hit %",
          "refId": "I"
        }
      ],
      "gridPos": {"h": 4, "w": 12, "x": 0, "y": 12},
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "thresholds"},
          "thresholds": {
            "steps": [
              {"color": "green", "value": 0},
              {"color": "yellow", "value": 5},
              {"color": "red", "value": 8}
            ]
          }
        }
      }
    },
    {
      "id": 6,
      "title": "üöÄ Application Uptime",
      "type": "stat",
      "targets": [
        {
          "expr": "application_uptime_seconds / 3600",
          "legendFormat": "Hours",
          "refId": "J"
        }
      ],
      "gridPos": {"h": 4, "w": 12, "x": 12, "y": 12},
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "palette-classic"},
          "unit": "h",
          "decimals": 1
        }
      }
    },
    {
      "id": 7,
      "title": "üìà Service Health Over Time",
      "type": "timeseries",
      "targets": [
        {
          "expr": "up",
          "legendFormat": "{{job}}",
          "refId": "K"
        }
      ],
      "gridPos": {"h": 6, "w": 24, "x": 0, "y": 16},
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "palette-classic"},
          "min": 0,
          "max": 1,
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "stepAfter"
          }
        }
      }
    }
  ]
}